{
  "name": "flat-iter",
  "version": "0.2.0",
  "description": "Deeply Iterate over Arrays. Like calling array.flat()[Symbol.iterator](), but without the memory cost",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "f": "npm run format",
    "format": "npx prettier --arrow-parens=avoid --print-width=120 --trailing-comma=none --write *.js *.ts",
    "t": "npm test",
    "test": "npm run test:js && npm run test:ts",
    "test:js": "node test.js",
    "test:ts": "npx ts-node test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielJDufour/flat-iter.git"
  },
  "keywords": [
    "array",
    "flat",
    "flatIter",
    "function",
    "item",
    "iter",
    "iterator",
    "symbol"
  ],
  "author": "Daniel J. Dufour",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/DanielJDufour/flat-iter/issues"
  },
  "homepage": "https://github.com/DanielJDufour/flat-iter#readme",
  "dependencies": {
    "iter-fun": "^0.1.2"
  },
  "devDependencies": {
    "flug": "^2.3.1"
  }
}
